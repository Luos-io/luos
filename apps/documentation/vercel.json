{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/documentation/:path*",
      "destination": "https://luos-documentation.vercel.app/documentation/:path*"
    },
    {
      "source": "/(blog|tutorials|faq)/:path*",
      "destination": "https://luos-documentation.vercel.app/documentation/$1/:path*"
    },
    {
      "source": "/app/:path*",
      "destination": "https://luos-services.vercel.app/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
