{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/docs/(luos-technology|tutorials|faq|blog)(.*)",
      "destination": "https://luos-documentation.vercel.app/$1$2",
      "statusCode": 301
    },
    {
      "source": "/docs/(assets/.*)",
      "destination": "https://luos-documentation.vercel.app/$1",
      "statusCode": 301
    }
  ]
}
