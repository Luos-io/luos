{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/docs/(luos-technology|tutorials|faq|blog)(.*)",
      "destination": "https://documentation-kordan1.vercel.app/$1$2"
    },
    {
      "source": "/docs/(assets/.*)",
      "destination": "https://documentation-kordan1.vercel.app/$1"
    }
  ]
}
