{
  "name": "luos",
  "version": "0.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\""
  },
  "devDependencies": {
    "eslint": "^8.23.1",
    "eslint-config-custom": "latest",
    "prettier": "^2.7.1",
    "turbo": "^1.4.7",
    "typescript": "^4.8.3"
  },
  "engines": {
    "pnpm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "dotenv-cli": "^6.0.0",
    "dotenv-vault": "^1.16.3"
  },
  "packageManager": "pnpm@7.13.5"
}
