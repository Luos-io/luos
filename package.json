{
  "name": "luos",
  "version": "0.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\""
  },
  "devDependencies": {
    "eslint": "^8.42.0",
    "eslint-config-custom": "latest",
    "prettier": "^2.8.8",
    "turbo": "^1.10.3",
    "typescript": "^5.1.3"
  },
  "engines": {
    "pnpm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "dotenv-cli": "^6.0.0",
    "dotenv-vault": "^1.16.3"
  },
  "packageManager": "pnpm@8.6.2"
}
