{
  "name": "luos",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv-vault pull && dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\""
  },
  "resolutions": {
    "@types/react": "17.0.43"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-custom": "latest",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "yarn": ">=1.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "dotenv-cli": "^5.1.0",
    "dotenv-vault": "^1.8.0"
  },
  "packageManager": "yarn@1.22.19"
}
