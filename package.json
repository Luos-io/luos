{
  "name": "luos",
  "version": "0.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\""
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-custom": "latest",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "pnpm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "dotenv-cli": "^7.3.0",
    "dotenv-vault": "^1.25.0"
  },
  "packageManager": "pnpm@8.11.0"
}
